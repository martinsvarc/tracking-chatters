# Environment Variables for Message Analyzer Platform
# Copy this file to .env and fill in your actual values

# NEON Postgres Database Connection String
# Format: postgresql://username:password@hostname:port/database?sslmode=require
DATABASE_URL=postgresql://your_username:your_password@your_host:5432/your_database?sslmode=require

# React App API URL (for development)
# Leave empty for production (uses relative URLs)
REACT_APP_API_URL=

# Server Port (for local development)
PORT=5000
